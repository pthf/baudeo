.top
  position relative
  display flex
  flex-wrap wrap
  justify-content center
  align-items center
  align-content center
  width 100%
  height 50vw
  z-index 1

  .Logo_Slider
    position relative
    width 30%
    height auto
    top 0
    left 0
    z-index 9

  .Logo_Slider img
    width 100%
    height auto
    top 0
    left 0
    z-index 9


/* Responsive queries */

@media (max-width: 1280px)
  .top-bar-list-item
    font-size .9rem

@media (max-width: 1024px)
  .top-bar-list-item
    font-size .8rem
